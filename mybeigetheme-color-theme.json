{
	"name": "mybeigetheme",
	"colors": {
	  "editor.background": "#1f1f1f",
	  "editor.foreground": "#eeffff",
	  "activityBarBadge.background": "#F3BE95",
	  "sideBarTitle.foreground": "#EFD3BF",
	  "statusBar.background": "#C17E70",  // Couleur de fond pour la barre d'état normale
	  "statusBar.foreground": "#EFD3BF",  // Couleur de texte pour la barre d'état
	  "statusBar.noFolderBackground": "#C17E70",  // Couleur de fond sans dossier ouvert
	  "statusBar.debuggingBackground": "#A0522D"  // Couleur de fond en mode débogage
	},
	
"tokenColors": [
	{
		"name": "Comment",
		"scope": [
			"comment",
			"punctuation.definition.comment"
		],
		"settings": {
			"fontStyle": "italic",
			"foreground": "#EFD3BF"
		}
	},
	{
		"name": "Variables",
		"scope": [
			"variable",
			"string constant.other.placeholder"
		],
		"settings": {
			"foreground": "#EEFFFF"
		}
	},
	{
		"name": "Parentheses, Braces, and Punctuation",
		"scope": [
		  "punctuation",
		  "meta.brace",
		  "punctuation.definition"
		],
		"settings": {
		  "foreground": "#EFD3BF"  
		}
	  },
	  {
		"name": "Invalid Elements and Language Methods",
		"scope": [
		  "invalid",
		  "invalid.illegal",
		  "variable.language",
		  "entity.name.tag",
		  "markup.deleted.git_gutter"
		],
		"settings": {
		  "foreground": "#F3BE95"  // Couleur nude/jaune pâle pour remplacer le rose
		}
	  },			
	{
		"name": "Colors",
		"scope": [
			"constant.other.color"
		],
		"settings": {
			"foreground": "#ffffff"
		}
	},
	{
		"name": "Invalid",
		"scope": [
			"invalid",
			"invalid.illegal"
		],
		"settings": {
			"foreground": "#C68054"
		}
	},
	{
		"name": "Keyword, Storage",
		"scope": [
			"keyword",
			"storage.type",
			"storage.modifier"
		],
		"settings": {
			"foreground": "#F3BE95"
		}
	},
	{
		"name": "Operator, Misc",
		"scope": [
			"keyword.control",
			"constant.other.color",
			"punctuation",
			"meta.tag",
			"punctuation.definition.tag",
			"punctuation.separator.inheritance.php",
			"punctuation.definition.tag.html",
			"punctuation.definition.tag.begin.html",
			"punctuation.definition.tag.end.html",
			"punctuation.section.embedded",
			"keyword.other.template",
			"keyword.other.substitution"
		],
		"settings": {
			"foreground": "#EFD3BF"
		}
	},
	{
		"name": "Tag",
		"scope": [
			"entity.name.tag",
			"meta.tag.sgml",
			"markup.deleted.git_gutter"
		],
		"settings": {
			"foreground": "#EFD3BF"
		}
	},
	{
		"name": "Function, Special Method",
		"scope": [
			"entity.name.function",
			"meta.function-call",
			"variable.function",
			"support.function",
			"keyword.other.special-method"
		],
		"settings": {
			"foreground": "#C68054"
		}
	},
	{
		"name": "Block Level Variables",
		"scope": [
			"meta.block variable.other"
		],
		"settings": {
			"foreground": "#EFD3BF"
		}
	},
	{
		"name": "Other Variable, String Link",
		"scope": [
			"support.other.variable",
			"string.other.link"
		],
		"settings": {
			"foreground": "#EFD3BF"
		}
	},
	{
		"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
		"scope": [
			"constant.numeric",
			"constant.language",
			"support.constant",
			"constant.character",
			"constant.escape",
			"variable.parameter",
			"keyword.other.unit",
			"keyword.other"
		],
		"settings": {
			"foreground": "#F3BE95"
		}
	},
	{
		"name": "String, Symbols, Inherited Class, Markup Heading",
		"scope": [
			"string",
			"constant.other.symbol",
			"constant.other.key",
			"entity.other.inherited-class",
			"markup.heading",
			"markup.inserted.git_gutter",
			"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
		],
		"settings": {
			"foreground": "#EFD3BF"
		}
	},
	{
		"name": "Class, Support",
		"scope": [
